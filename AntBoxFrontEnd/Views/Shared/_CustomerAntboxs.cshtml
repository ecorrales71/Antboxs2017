@model IEnumerable<AntBoxFrontEnd.Services.AntBoxes.AntBoxResponse>

@foreach (var item in Model)
{
            

            <div class="col-sm-12 mb20 antbox-item">
                <div class="card card-border-dashed card-border-color magnific-wrap">
                    <div class="row">
                        <div class="col-sm-4 col-xs-12">
                            <figure class="figure">
                                <img src="@item.imagen" alt="">
                                @if (item.Status == "free")
                                {
                                    <div class="sticker_mov completed">Entregado</div>
                                }
                                <div class="mask mask-dark">
                                    <nav>
                                        <a class="icon icon-inverse icon-size-1 icon-theme icon-rounded magnific" title="@Html.DisplayFor(modelItem => item.Name) - @Html.DisplayFor(modelItem => item.Folio)" href="@item.imagen">
                                            <i class="fa fa-search"></i>
                                        </a>
                                    </nav>
                                </div>
                            </figure>
                        </div>
                        <div class="col-sm-8 col-xs-12 antboxs_txt" >
                            <div>
                                <h4><span class="name-antbox">
                                    @Html.DisplayFor(modelItem => item.Name)</span> - @Html.DisplayFor(modelItem => item.Folio) @Html.DisplayFor(modelItem => item.EnDeposito)
                                </h4>
                            </div>
                            <p class="description-antbox">@Html.DisplayFor(modelItem => item.Descriptionv)</p>
                            <div class="checkboxFive">
                                <input type="checkbox" value="1" id="checkboxFiveInput-@item.Id" class="select-antbox-check" name="" data-id="@Html.DisplayFor(modelItem => item.Id)" data-serial="@Html.DisplayFor(modelItem => item.Serial)" data-folio="@Html.DisplayFor(modelItem => item.Folio)">
                                <label for="checkboxFiveInput-@item.Id" class="select-antbox" data-folio="@Html.DisplayFor(modelItem => item.Folio)"></label>
                            </div>
                            <button type="button" class="btn btn-primary btn-sm pull-right ml20 solicitar" data-folio="@Html.DisplayFor(modelItem => item.Folio)" data-id="@Html.DisplayFor(modelItem => item.Id)" data-status="@Html.DisplayFor(modelItem => item.Status)">Solicitar</button>
                            <button type="button" class="btn btn-primary btn-sm pull-right btn_blue editar" data-toggle="modal"  data-target="#editar">Editar</button>
                        </div>
                    </div>
                </div>
            </div>
