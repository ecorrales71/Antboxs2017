<script type="text/javascript" src="~/Scripts/validaciones.js"></script>
<script>
    $(function () {
        //Validar letras y espacios
        $('#email, #email2').keypress(function (e) {
            var txt = String.fromCharCode(e.which);
            if (!txt.match(/[A-Za-z0-9ñÑáéíóúÁÉÍÓÚ@@\-\_&'.]/)) {
                return false;
            }
        }).on('keydown', function (e) {
            if (e.keyCode == 8)
                $(this).trigger('keypress');
        });
        $('#zipCode').keypress(function (e) {
            var txt = String.fromCharCode(e.which);
            if (!txt.match(/[0-9]/)) {
                return false;
            }
        }).on('keydown', function (e) {
            if (e.keyCode == 8)
                $(this).trigger('keypress');
        });
        function isValidEmail(mail) {
            return /^\w+([\.\+\-]?\w+)*@@\w+([\.-]?\w+)*(\.\w{2,4})+$/.test(mail);
        }
        $('.registro-modal').click(function () {
            $('#registro').find('#verifoption').val(1);
            $('#registro').modal('show');
        })
        $('#registro').on('shown.bs.modal', function () {
            $("#txtname").focus();
        })
        $('.recuperar-password').click(function () {
            $('.div-cont').hide();
            $('#FormReset')[0].reset();
            $('#login').find('.modal-title').text('Recuperar contraseña');
            $('.div-reset').show();
        });
        $('#btnVolver').click(function () {
            $('.div-cont').hide();
            $('#LoginForm')[0].reset();
            $('#login').find('.modal-title').text('Entrar a mi cuenta');
            $('.div-login').show();
        });
        $('#btnVolverRestore').click(function () {
            $('.div-cont').hide();
            $('#FormReset')[0].reset();
            $('#login').find('.modal-title').text('Recuperar contraseña');
            $('.div-login').show();
        });
        $('#btnEmviarReset').click(function(e){
            e.preventDefault();
            if ($('#FormReset').valid())
            {
                showLoad(1);
                $.ajax({
                    type: 'POST',
                    url: '@Url.Action("ResetPasswordV", "Account")',
                    data: { email : $('#emailres').val() },
                    success: function (result) {
                        if (result.success == false) {
                            alert('E-mail no registrado');
                            showLoad(0);
                        }
                        else {
                            alert('Revisa tu cuenta de e-mail, hemos enviado tu codigo para el cambio de contraseña.');
                            $('.div-cont').hide();
                            $('#FormRestore')[0].reset();
                            $('.div-restore').show();
                            showLoad(0);
                        }
                    },
                    error: function (emp) {
                        showLoad(0);
                    }
                });
            }
            
        });
        $('#btnEmviarRestore').click(function (e) {
            e.preventDefault();
            if ($('#FormRestore').valid())
            {
                showLoad(1);
                $.ajax({
                    type: 'POST',
                    url: '@Url.Action("RestorePassword", "Account")',
                    data: $('#FormRestore').serialize(),
                    success: function (result) {
                        if (result.success == false) {
                            alert('Codigo erroneo')
                            showLoad(0);
                        }
                        else {
                            showLoad(0);
                            $('#login').find('.modal-title').text('Entrar a mi cuenta');
                            alert('Password cambiado correctamente');
                            $('.div-cont').hide();
                            $('.div-login').show();
                        }
                    },
                    error: function (emp) {
                        showLoad(0);
                    }
                });
            }
        });
        $('#FormReset').validate({
            rules: {
                emailres: {
                    required: true,
                    email: true
                }
            },
            highlight: function (element) {
                $(element).closest('.form-group').addClass('has-error');
            },
            unhighlight: function (element) {
                $(element).closest('.form-group').removeClass('has-error');
            },
            errorElement: 'span',
            errorClass: 'help-block',
            errorPlacement: function (error, element) {
                if (element.parent('.input-group').length) {
                    error.insertAfter(element.parent());
                } else {
                    error.insertAfter(element);
                }
            }
        });
        $('#FormRestore').validate({
            rules: {
                code: {
                    required: true
                },
                Newpassword: {
                    required: true,
                    minlength: 8,
                    maxlength: 20
                },
                Newpassword1: {
                    required: true,
                    equalTo: "#Newpassword",
                    minlength: 8,
                    maxlength: 20
                }
            },
            highlight: function (element) {
                $(element).closest('.form-group').addClass('has-error');
            },
            unhighlight: function (element) {
                $(element).closest('.form-group').removeClass('has-error');
            },
            errorElement: 'span',
            errorClass: 'help-block',
            errorPlacement: function (error, element) {
                if (element.parent('.input-group').length) {
                    error.insertAfter(element.parent());
                } else {
                    error.insertAfter(element);
                }
            }
        });
    })
    $("#usernamer").keypress(function (e) {
        if (e.which == 13) {
            $("#passwordr").focus();
        }
    })
    $("#passwordr").keypress(function (e) {
        if (e.which == 13) {
            $('#btnLogin').trigger("click");
        }
    })
    $('#btnLogin').click(function () {
        $.ajax({
            type: 'POST',
            url: '@Url.Action("AntBoxLoginAjax", "Account")',
            data: $('#LoginForm').serialize(),
            success: function (result) {
                if (result.success == false) {
                    $('#body-content').prepend(
                    '<div class="alert alert-warning" role="alert" id="LoginFailAlert">Correo electrónico o Password no valido.</div>');
                    window.setTimeout(function ()
                    { $("#LoginFailAlert").alert('close'); }, 2000);
                }
                else {
                    window.location.href = '@Url.Action("Index", "Customer")';
                }
            },
            error: function (emp) {
                $('#body-content').prepend(
                    '<div class="alert alert-danger fade-in" role="alert" id="LoginErrorAlert">Error al realizar el login. Intente nuevamente.</div>'
                    );
                window.setTimeout(function ()
                { $("#LoginErrorAlert").alert('close'); }, 2000);

            }
        });
