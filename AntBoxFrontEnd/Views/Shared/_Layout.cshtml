<script>
        $('#testAjax').click(function () {
            $.ajax({
                type: 'POST',
                url: 'Account/AntBoxRegister',
                data: {},
                success: function (emp) {
                    $('#body-content').prepend(
                        '<div class="alert alert-success" id="success-alert">Login exitoso</div>');
                    window.setTimeout(function ()
                    { $("#success-alert").alert('close'); }, 2000);
                    
                },
                error: function (emp) {
                    $('#body-content').prepend(
                        '<div class="alert alert-danger fade-in" id="error-alert">Login fallido</div>'
                        );
                    window.setTimeout(function ()
                    { $("#error-alert").alert('close'); }, 2000);
                    
                }
            });
        });

</script>